{
	"redirects": [
        { 
            "source": "/:path*",
            "has": [
                {
                    "type": "host",
                    "value": "ecl-it.cz"
                }
            ],
            "destination": "https://www.ecl-it.cz/:path*",
            "permanent": true
        }
    ],
    "rewrites": [
        { "source": "/(.*)", "destination": "/index.html" }
    ]
}
